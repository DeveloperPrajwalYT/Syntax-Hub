local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "DeathXUltimate", HidePremium = false,IntroText = "SyntaXDeath Scripts Hub V1", SaveConfig = true, ConfigFolder = "Scripts"})

local TabBloxFruits = Window:MakeTab({
    Name = "Blox Fruits",
    Icon = "https://image.shutterstock.com/image-vector/white-check-mark-icon-no-260nw-1430592379.jpg",
    PremiumOnly = false
})

OrionLib:MakeNotification({
    Name = "HEY",
    Content = "linktr.ee/codeweb.com !",
    Image = "rbxassetid://4483345998",
    Time = 5
})

local SectionBloxFruits = TabBloxFruits:AddSection({
    Name = "Blox Fruits Scripts"
})

-- Add Blox Fruits scripts
TabBloxFruits:AddButton({
    Name = "RedZ",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/BloxFruits/main/redz9999"))()
        print("button pressed")
    end
})

-- Add more Blox Fruits scripts here...

